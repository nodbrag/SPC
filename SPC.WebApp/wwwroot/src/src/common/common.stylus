// px2rem
px2rem(designpx)
    $rem = 16px
    return (designpx / $rem)rem

// 修改element样式
.el-table th
    background #2673FF!important
    color #fff
    font-size 12px
    padding 0
    font-family '方正黑体简体'
    text-align center
.el-table td
    font-size 12px
    padding 5px 0
    border-bottom 2px solid #E2E2E2
    font-family '方正细黑一简体'
    text-align center
.el-table .cell
    line-height 34px
    height 34px
.el-button--primary
    background #2673FF
.el-card__body
